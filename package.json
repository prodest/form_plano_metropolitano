{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "start": "concurrently \"npm run ntsc:w\" \"npm run lite\" \"npm run gulp:build:copy-static\"",
    "e2e": "ntsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./src/app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "ntsc && concurrently \"ntsc -w\" \"karma start karma.conf.js\"",
    "test-once": "ntsc && karma start karma.conf.js --single-run",
    "gulp:build:copy-static": "gulp build:copy-static",
    "ntsc": "ntsc",
    "ntsc:w": "ntsc -w",
    "copy": "gulp build:copy-static"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "@prodest/mapeandoes-typings": "^1.0.11",
    "angular-in-memory-web-api": "~0.2.4",
    "babel-polyfill": "^6.22.0",
    "core-js": "^2.4.1",
    "oidc-client": "^1.2.2",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-bundle-assets": "^2.27.1",
    "gulp-clean": "^0.3.2",
    "gulp-minify": "0.0.14",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-systemjs-builder": "^0.15.0",
    "gulp-typescript": "^3.1.4",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "ntypescript": "^1.201609302242.1",
    "path": "^0.12.7",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",
    "systemjs-builder": "^0.15.34",
    "tslint": "^3.15.1",
    "typescript": "~2.0.10"
  },
  "repository": {}
}
